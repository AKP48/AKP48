// Just so you know, comments aren't valid JSON, so you should probably
// remove any you see laying around.
{
    "#botplace": {
        "users": {
            "Nickname!Username@hostname.com": {
                "powerLevel": 9001 // User's power level. See example server config for default power levels.
            }
        }
    },
    "#channel": {
        "users": {
            "Nickname!Username@hostname.com": {
                "powerLevel": 1000
            }
        }
    },
    "global": { // The global channel. This is used for private messages.
        "users": {
            "Nickname!Username@hostname.com": {
                "powerLevel": 9001
            }
        }
    }
}
